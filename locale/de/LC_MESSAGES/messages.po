#, python-brace-format
msgid "The next alarm {room} starts in {minutes} minutes at {time}."
msgstr "Der nächste Wecker wird in {minutes} Minuten um {time} {room} klingeln."

#: action-alarmclock.py:186
#, python-brace-format
msgid "The next alarm {room} starts at {time}."
msgstr "Der Wecker wird um {time} {room} klingeln."

