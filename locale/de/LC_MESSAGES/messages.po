msgstr[1] "Du hast {num} Wecker {alarms} {filler} verpasst."

#: action-alarmclock.py:210
msgid "and more"
msgstr "und weitere"

#: action-alarmclock.py:225
#, python-brace-format
msgid "Do you really want to delete the alarm {day} at {time} {room}?"
